<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingTop="20dp"
    android:paddingLeft="20dp"
    android:paddingRight="20dp"
    tools:context="aditya.services.adityagate.res_civilFragment">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/faculty_name1"
        android:textSize="25sp"
        android:textStyle="bold"
        android:id="@+id/textView4"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textColor="@color/material_grey_900" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/faculty_no1"
        android:autoLink="phone"
        android:textSize="25sp"
        android:textStyle="bold"
        android:id="@+id/textView5"
        android:textColor="@color/material_grey_900"
        android:layout_alignTop="@+id/textView4"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_marginTop="10dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/faculty_name2"
        android:textSize="25sp"
        android:textStyle="bold"
        android:id="@+id/textView6"
        android:textColor="@color/material_grey_900"
        android:layout_below="@+id/textView4"
        android:layout_alignLeft="@+id/textView4"
        android:layout_alignStart="@+id/textView4" />

    <TextView
        android:layout_marginTop="10dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/faculty_name3"
        android:textSize="25sp"
        android:textStyle="bold"
        android:id="@+id/textView7"
        android:textColor="@color/material_grey_900"
        android:layout_below="@+id/textView6"
        android:layout_alignLeft="@+id/textView6"
        android:layout_alignStart="@+id/textView6" />

    <TextView
        android:layout_marginTop="10dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/faculty_name4"
        android:textSize="25sp"
        android:textStyle="bold"
        android:id="@+id/textView8"
        android:textColor="@color/material_grey_900"
        android:layout_below="@+id/textView7"
        android:layout_alignLeft="@+id/textView7"
        android:layout_alignStart="@+id/textView7" />

    <TextView
        android:layout_marginTop="10dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/faculty_no2"
        android:autoLink="phone"
        android:textSize="25sp"
        android:textStyle="bold"
        android:id="@+id/textView9"
        android:textColor="@color/material_grey_900"
        android:layout_below="@+id/textView5"
        android:layout_alignRight="@+id/textView5"
        android:layout_alignEnd="@+id/textView5" />

    <TextView
        android:layout_marginTop="10dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/faculty_no3"
        android:autoLink="phone"
        android:textSize="25sp"
        android:textStyle="bold"
        android:id="@+id/textView10"
        android:textColor="@color/material_grey_900"
        android:layout_below="@+id/textView9"
        android:layout_alignRight="@+id/textView9"
        android:layout_alignEnd="@+id/textView9" />

    <TextView
        android:layout_marginTop="10dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/faculty_no4"
        android:autoLink="phone"
        android:textSize="25sp"
        android:textStyle="bold"
        android:id="@+id/textView11"
        android:textColor="@color/material_grey_900"
        android:layout_below="@+id/textView10"
        android:layout_alignRight="@+id/textView10"
        android:layout_alignEnd="@+id/textView10" />

</RelativeLayout>
